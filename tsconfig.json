{
  "compilerOptions": {
    "sourceMap": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "target": "es5",
    "jsx": "react",
    "lib": [
      "dom",
      "es6",
      "es2015.collection",
      "es2015.iterable"
    ],
    "downlevelIteration": true,
    "importHelpers": true,
  },
  "include": [
    "src"
  ],
  "compileOnSave": false
}
